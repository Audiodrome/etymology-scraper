{
  "name": "etymology-scraper",
  "version": "1.0.0",
  "description": "A web app for readers who would like to get feedback about a webpage's content and the etymology thereof.",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/",
    "lint": "eslint .",
    "bundle": "browserify -t babelify client/main.js -o server/public/bundle.js",
    "start": "node server/index.js",
    "watch:client": "watchify -t babelify client/main.js -v -o server/public/bundle.js",
    "watch:server": "nodemon server/index.js",
    "watch": "run-p watch:client watch:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielduffield/etymology-scraper.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielduffield/etymology-scraper/issues"
  },
  "homepage": "https://github.com/danielduffield/etymology-scraper#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "node-readability": "^3.0.0",
    "readability": "^0.1.0",
    "readability-node": "^0.1.0",
    "sanitizer": "^0.1.3"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.0",
    "prettier": "1.8.2",
    "prettier-eslint": "^8.2.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "nodemon": "^1.12.1",
    "watchify": "^3.9.0"
  }
}
